            {
              "name": "GetPlayerData",
              "event": [
                {
                  "listen": "test",
                  "script": {
                    "exec": [
                      "const response = pm.response.json();\r",
                      "\r",
                      "if (!response) {\r",
                      "    console.error(\"Response did not contain valid JSON\");\r",
                      "    return;\r",
                      "}\r",
                      "\r",
                      "// Store entire response as JSON string\r",
                      "pm.environment.set(\"player_a_info\", JSON.stringify(response));\r",
                      "console.log(\"Player A full info stored\");\r",
                      "\r",
                      "// Recursive function to flatten nested objects/arrays\r",
                      "function flattenObject(obj, prefix = \"player_a\") {\r",
                      "    for (const key in obj) {\r",
                      "        if (!obj.hasOwnProperty(key)) continue;\r",
                      "        const value = obj[key];\r",
                      "        const envVarName = `${prefix}_${key}`;\r",
                      "\r",
                      "        if (value === null || value === undefined) {\r",
                      "            pm.environment.set(envVarName, \"\");\r",
                      "            console.log(`Set ${envVarName}: \"\"`);\r",
                      "        } else if (typeof value === \"object\") {\r",
                      "            if (Array.isArray(value)) {\r",
                      "                // Store array as JSON string\r",
                      "                pm.environment.set(envVarName, JSON.stringify(value));\r",
                      "                console.log(`Set ${envVarName}: ${JSON.stringify(value)}`);\r",
                      "            } else {\r",
                      "                // Nested object â†’ recurse\r",
                      "                flattenObject(value, envVarName);\r",
                      "            }\r",
                      "        } else {\r",
                      "            pm.environment.set(envVarName, String(value));\r",
                      "            console.log(`Set ${envVarName}: ${value}`);\r",
                      "        }\r",
                      "    }\r",
                      "}\r",
                      "\r",
                      "// Flatten all top-level and nested properties\r",
                      "flattenObject(response);\r",
                      ""
                    ],
                    "type": "text/javascript",
                    "packages": {},
                    "requests": {}
                  }
